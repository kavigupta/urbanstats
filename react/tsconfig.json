{
    "compilerOptions": {
        "strict": true,
        "lib": [
            "ES2021",
            "DOM"
        ],
        "jsx": "react",
        "esModuleInterop": true,
        "target": "ES2022",
        "module": "CommonJS",
        "resolveJsonModule": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "allowUnreachableCode": false,
        "skipLibCheck": true,
        "noEmit": true
    }
}